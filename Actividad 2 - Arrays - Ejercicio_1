package prueba_arrays;

import java.util.ArrayList;
import java.util.Scanner;

public class Prueba_arrays {

    public static void main(String[] args) {

        /*Ejercicio 1
 Crear un array de tipo Integer de 10 posiciones que permita al usuario cargar los diez valores por
teclado.
 Luego que imprima esos valores.*/
        
        int array[] = new int[10];
        Scanner usu = new Scanner(System.in);
        
        for(int i=0; i<10; i++){
            System.out.println("Ingrese un numero: ");
            array[i] = usu.nextInt();
        }
       
        for(int i=0; i<10; i++){
            System.out.println("Los numeros ingresados son: "+array[i]);
        }
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
    }

}
